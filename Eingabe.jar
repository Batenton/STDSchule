import java.util.Scanner;

public class Eingabe
{
    public static void Eingabe(String args[])
    {
    Scanner meinScanner = new Scanner(System.in);
    String name;
    int zahl;
    System.out.print("Bitte geben Sie Ihren Namen ein");
    
    name = meinScanner.nextLine();
    
    System.out.print("Bitte geben Sie Ihre Liebelingszahl ein: ");
    
    zahl = meinScanner.nextInt();
    
    System.out.println("Die Lieblingszahl von " + name + " ist " + zahl);
    }
}
